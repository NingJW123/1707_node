<%- include head.ejs %>
<%- include nav.ejs %>

<div class="container" style="margin-top: 70px">
  <div class="row">
    <% for(let i=0,len=albums.length;i<len;i++){ %>
      <div class="col-sm-6 col-md-3">
        <div class="thumbnail">
          <img src="/images/mimi.jpg" alt="<%= albums[i] %>">
          <div class="caption">
            <p><%= albums[i] %></p>
          </div>
        </div>
      </div>
    <% } %>
  </div>
</div>

